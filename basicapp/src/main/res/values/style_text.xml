    <style name="edit_style">
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/clr_333</item>
        <item name="android:textColorHint">@color/clr_ccc</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="edit_style_telphone" parent="edit_style">
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">11</item>
    </style>

    <style name="edit_style_number_20" parent="edit_style">
        <item name="android:inputType">phone</item>
        <item name="android:maxLength">20</item>
    </style>

    <style name="edit_style_id_card" parent="edit_style">
        <item name="android:digits">1234567890Xx</item>
        <item name="android:maxLength">18</item>
    </style>


    <style name="edit_style_name" parent="edit_style">
        <item name="android:inputType">text</item>
        <item name="android:maxLength">20</item>
    </style>

    <style name="edit_style_password" parent="edit_style">
        <item name="android:inputType">textPassword</item>
        <item name="android:maxLength">20</item>
    </style>

    <style name="edit_style_visible_password" parent="edit_style">
        <item name="android:inputType">textVisiblePassword</item>
        <item name="android:maxLength">20</item>
    </style>

    <style name="edit_style_pay_password" parent="edit_style">
        <item name="android:inputType">numberPassword</item>
        <item name="android:maxLength">6</item>
    </style>

    <style name="edit_style_verify_code" parent="edit_style">
        <item name="android:inputType">number</item>
        <item name="android:maxLength">6</item>
    </style>

    <style name="edit_style_address" parent="edit_style">
